}

@article{rousseauxHumanMilkOligosaccharides2021a,
  title = {Human {{Milk Oligosaccharides}}: {{Their Effects}} on the {{Host}} and {{Their Potential}} as {{Therapeutic Agents}}},
  shorttitle = {Human {{Milk Oligosaccharides}}},
  author = {Rousseaux, Ana{\"i}s and Brosseau, Carole and Le Gall, Sophie and Piloquet, Hugues and Barbarot, S{\'e}bastien and Bodinier, Marie},
  year = 2021,
