@article{kuipersDifferencesPretermTerm2011,
  title = {Differences in Preterm and Term Milk Fatty Acid Compositions May Be Caused by the Different Hormonal Milieu of Early Parturition},
  author = {Kuipers, Remko S. and Luxwolda, Martine F. and {Dijck-Brouwer}, D. A. Janneke and Muskiet, Frits A. J.},
  year = 2011,
  month = dec,
  journal = {Prostaglandins, Leukotrienes and Essential Fatty Acids},
  volume = {85},
  number = {6},
  pages = {369--379},
  issn = {0952-3278},
  doi = {10.1016/j.plefa.2011.08.001},
  urldate = {2026-01-07},
  abstract = {Introduction The hormonal milieus of pregnancy and lactation are driving forces of nutrient fluxes supporting infant growth and development. The decrease of insulin sensitivity with compensatory hyperinsulinemia with advancing gestation, causes adipose tissue lipolysis and hepatic de novo lipogenesis (DNL). Subjects and methods We compared fatty acid (FA) contents and FA-indices for enzyme activities between preterm (28--36 weeks) and term (37--42) milks, and between colostrum (2--5 days), transitional (6--15) and mature (16--56) milks. We interpreted FA differences between preterm and term milks, and their changes with lactation, in terms of the well known decrease of insulin sensitivity during gestation and its subsequent postpartum restoration, respectively. Results Compared with term colostrum, preterm colostrum contained higher indices of DNL in the breast (DNL-breast) and medium chain saturated-FA (MCSAFA), and lower DNL-liver and monounsaturated-FA (MUFA). Preterm milk also had higher docosahexaenoic acid (DHA) in colostrum and transitional milk and higher arachidonic acid (AA) in mature milk. Most preterm-term differences vanished with advancing lactation. In both preterm and term milks, DNL-breast and MCSAFA increased with advancing lactation, while DNL-liver, MUFA, long chain SAFA and AA decreased. DHA decreased in term milk. MUFA was inversely related to MCSAFA in all samples, correlated inversely with PUFA in colostrum and transitional milks, but positively in mature milk. MCSAFA correlated inversely with PUFA in mature milk. Conclusion Higher maternal insulin sensitivity at preterm birth may be the cause of lower MUFA (a proxy for DNL-liver) and higher MCSAFA (a proxy for DNL-breast) in preterm colostrum, compared with term colostrum. Restoring insulin sensitivity after delivery may be an important driving force for milk FA-changes in early lactation.},
  langid = {english},
  keywords = {Fatty acids long chain polyunsaturated fatty acids (LCP),Insulin sensitivity,lipogenesis,Preterm delivery preterm and term human milk},
  annotation = {abstractTranslation: はじめに 妊娠と授乳のホルモン環境は、乳児の成長と発育をサポートする栄養の流れの原動力です。妊娠の進行に伴う代償性高インスリン血症によるインスリン感受性の低下は、脂肪組織の脂肪分解と肝臓の新規脂肪生成（DNL）を引き起こします。対象と方法 早産乳（28 ～ 36 週）と正期産乳（37 ～ 42 週）の間、および初乳（2 ～ 5 日）、移行期乳（6 ～ 15 週）および成熟乳（16 ～ 56 週）の間で、脂肪酸（FA）含有量と酵素活性の FA インデックスを比較しました。早産乳と正期産乳の間のFAの違い、および授乳に伴うそれらの変化を、それぞれよく知られている妊娠中のインスリン感受性の低下とその後の産後の回復という観点から解釈しました。結果 正期初乳と比較して、早産初乳には胸部の DNL (DNL-breast) および中鎖飽和 FA (MCSAFA) の指数が高く、DNL-肝臓および一価不飽和 FA (MUFA) の指数が低かった。早産乳には、初乳と移行乳のドコサヘキサエン酸 (DHA) が多く、成熟乳にはアラキドン酸 (AA) が多く含まれていました。ほとんどの早産期の差異は授乳の進行とともに消失しました。早産乳と正期産乳の両方において、DNL-乳房とMCSAFAは授乳の進行とともに増加しましたが、DNL-肝臓、MUFA、長鎖SAFA、AAは減少しました。定期ミルクではDHAが減少しました。 MUFA はすべてのサンプルで MCSAFA と逆相関し、初乳および移行乳では PUFA と逆相関しましたが、成熟乳では正の相関がありました。 MCSAFA は、成熟乳中の PUFA と逆相関しました。結論 早産時の母体のインスリン感受性が高いことは、正期初乳と比較して、早産初乳の MUFA (DNL 肝臓の代用) が低く、MCSAFA (DNL 乳房の代用) が高くなる原因である可能性があります。分娩後のインスリン感受性の回復は、授乳初期における乳汁 FA 変化の重要な原動力となる可能性があります。},
  file = {C:\Users\user\OneDrive_HU\Zotero\1_Paper\Press\2025_MCthesis\Kuipers et al. - 2011 - Differences in preterm and term milk fatty acid compositions may be caused by the different hormonal.pdf}
}

