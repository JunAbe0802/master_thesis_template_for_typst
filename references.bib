  langid = {english},
  keywords = {Developmental biology,Genetics,Microbiome,Neonatology},
  file = {C:\Users\user\OneDrive_HU\Zotero\1_Paper\Press\2025_MCthesis\Matsuki et al. - 2016 - A key genetic factor for fucosyllactose utilization affects infant gut microbiota development.pdf}
}

