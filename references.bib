@article{borewiczEffectPrebioticFortified2019,
  title = {The Effect of Prebiotic Fortified Infant Formulas on Microbiota Composition and Dynamics in Early Life},
  author = {Borewicz, Klaudyna and {Suarez-Diez}, Maria and Hechler, Christine and Beijers, Roseriet and {de Weerth}, Carolina and Arts, Ilja and Penders, John and Thijs, Carel and Nauta, Arjen and Lindner, Cordula and Van Leusen, Ellen and Vaughan, Elaine E. and Smidt, Hauke},
  year = 2019,
  month = feb,
  journal = {Sci Rep},
  volume = {9},
  number = {1},
  pages = {2434},
  publisher = {Nature Publishing Group},
  issn = {2045-2322},
  doi = {10.1038/s41598-018-38268-x},
  urldate = {2026-01-06},
  abstract = {Gastrointestinal (GI)~microbiota composition differs between breastfed and formula-fed infants. Today's infant formulas are often fortified with prebiotics to better mimic properties of human milk with respect to its effect on GI microbiota composition and function. We used Illumina HiSeq sequencing of PCR-amplified 16S rRNA gene fragments to investigate the composition of faecal microbiota in 2--12 week old infants receiving either breastmilk, infant formulas fortified with prebiotics, or mixed feeding. We compared these results with results from infants fed traditional formulas used in the Netherlands in 2002--2003, which contained no added prebiotics. We showed that today's formulas supplemented with either scGOS (0.24--0.50\,g/100\,ml) or scGOS and lcFOS (at a 9:1 ratio; total 0.6\,g/100\,ml) had a strong bifidogenic effect as compared to traditional formulas, and they also resulted in altered patterns of microbial colonisation within the developing infant gastrointestinal tract. We identified three microbial states (or developmental stages) in the first 12 weeks of life, with a gradual transition pattern towards a bifidobacteria dominated state. In infants receiving only fortified formulas, this transition towards the bifidobacteria dominated state was accelerated, whereas in infants receiving mixed feeding the transition was delayed, as compared to exclusively breastfed infants.},
  copyright = {2019 The Author(s)},
  langid = {english},
  keywords = {Microbiome,Microbiota},
  annotation = {abstractTranslation: 胃腸（GI）微生物叢の組成は、母乳で育てられた乳児と粉ミルクで育てられた乳児では異なります。今日の乳児用ミルクは、消化管微生物叢の組成と機能に対する影響に関して母乳の特性をよりよく模倣するために、プレバイオティクスが強化されていることがよくあります。私たちは、PCR増幅した16S rRNA遺伝子断片のIllumina HiSeqシーケンスを使用して、母乳、プレバイオティクスで強化された乳児用ミルク、または混合栄養のいずれかを与えられている生後2〜12週の乳児の糞便微生物叢の組成を調査しました。私たちはこれらの結果を、2002年から2003年にオランダで使用され、プレバイオティクスが添加されていない伝統的な粉ミルクを与えられた乳児の結果と比較しました。私たちは、scGOS (0.24 ～ 0.50 g/100 ml)、または scGOS と lcFOS (9:1 の比率、合計 0.6 g/100 ml) を添加した現在の処方には、従来の処方と比較して強いビフィズス形成効果があり、また、発育中の乳児の胃腸管内での微生物定着パターンの変化をもたらすことを示しました。私たちは、ビフィズス菌優位の状態への段階的な移行パターンを持つ、生後 12 週間の 3 つの微生物の状態 (または発達段階) を特定しました。強化粉ミルクのみを与えられた乳児では、ビフィズス菌優勢状態への移行が加速されましたが、混合栄養を受けている乳児では、完全母乳で育てられた乳児と比較して、移行が遅れました。},
  file = {C:\Users\user\OneDrive_HU\Zotero\1_Paper\Borewicz et al. - 2019 - The effect of prebiotic fortified infant formulas on microbiota composition and dynamics in early li.pdf}
}

@article{borrego-ruizEarlylifeGutMicrobiome2025,
  title = {Early-Life Gut Microbiome Development and Its Potential Long-Term Impact on Health Outcomes},
  author = {{Borrego-Ruiz}, Alejandro and Borrego, Juan J.},
  year = 2025,
  month = apr,
  journal = {mrr},
  volume = {4},
  number = {2},
  pages = {N/A-N/A},
  publisher = {OAE Publishing Inc.},
  issn = {ISSN 2771-5965 (Online)},
  doi = {10.20517/mrr.2024.78},
  urldate = {2026-01-06},
  abstract = {The initial gut colonization of the infant plays a pivotal role in shaping the immune system, developing the intestinal tract, and influencing host metabolism, all of which are strongly influenced by several determinants, such as gestational age at birth, mode of delivery, neonatal feeding practices, early-life stress (ELS), and exposure to perinatal antibiotics. However, resulting gut microbiome (GM) dysbiosis may alter this developmental programming, leading to long-term adverse health outcomes. This narrative review synthesizes current knowledge on early-life GM development and its long-term impact on health. Specifically, it addresses how early-life GM dysbiosis may affect the trajectory of physiological processes, predisposing individuals to conditions such as allergic diseases, metabolic disorders, type 1 diabetes, inflammatory bowel disorders, and atherosclerotic cardiovascular diseases. In addition, it examines the influence of probiotic and prebiotic supplementation during pregnancy and early life in shaping infant GM composition, as well as the impact of ELS-induced GM dysbiosis on mental health. Recent research suggests that the early-life microbiota initiates long-lasting effects, and inadequate or insufficient microbial exposure triggers inflammatory responses associated with several physiological conditions. Although several studies have reported a connection between ELS and the GM during both prenatal and postnatal periods, a unified microbiome signature linked to either prenatal or postnatal stress remains to be fully elucidated. Thus, future studies are needed to establish causality and determine whether modifiable factors affecting the GM could be targeted to improve gut health, especially in children exposed to contextual stress or adverse conditions.},
  langid = {english},
  file = {C:\Users\user\OneDrive_HU\Zotero\1_Paper\Press\2025_MCthesis\Borrego-Ruiz と Borrego - 2025 - Early-life gut microbiome development and its potential long-term impact on health outcomes.pdf}
}

@article{florisHumanMilkFatty2020,
  title = {Human Milk Fatty Acid Profile across Lactational Stages after Term and Preterm Delivery: {{A}} Pooled Data Analysis},
  shorttitle = {Human Milk Fatty Acid Profile across Lactational Stages after Term and Preterm Delivery},
  author = {Floris, L. M. and Stahl, B. and {Abrahamse-Berkeveld}, M. and Teller, I. C.},
  year = 2020,
  month = may,
  journal = {Prostaglandins, Leukotrienes and Essential Fatty Acids},
  volume = {156},
  pages = {102023},
  issn = {0952-3278},
  doi = {10.1016/j.plefa.2019.102023},
  urldate = {2026-01-07},
  abstract = {Background Lipids in human milk (HM) provide the majority of energy for developing infants, as well as crucial essential fatty acids (FA). The FA composition of HM is highly variable and influenced by multiple factors. We sought to increase understanding of the variation in HMFA profiles and their development over the course of lactation, and after term and preterm delivery, using a pooled data analysis. Objective To review the literature and perform a pooled data analysis to qualitatively describe an extensive FA profile (36 FAs) in term and preterm colostrum, transitional - and mature milk up to 60 days postpartum. Design A Medline search was conducted for HMFA profile data following term or preterm delivery. The search was confined to English language papers published between January 1980 and August 2018. Studies reporting original data, extensive FA profiles in HM from healthy mothers were included. Weighted least squares (WLS) means were calculated from the pooled data using random or fixed effect models. Results Our pooled data analysis included data from 55 studies worldwide, for a total of 4374 term milk samples and 1017 preterm milk samples, providing WLS means for 36 FAs. Patterns in both term and preterm milk were apparent throughout lactation for some FAs: The most abundant FAs (palmitic, linoleic and oleic acid) remained stable over time, whereas several long-chain polyunsaturated FAs (including ARA and DHA) seemed to decrease and short- and medium-chain FAs increased over time. Conclusions High heterogeneity between individual studies was observed for the reported levels of some FAs, whereas other FAs were remarkably consistent between studies. Our pooled data suggests that specific FA categories fluctuate according to distinct patterns over the course of lactation; many of these patterns are comparable between term and preterm milk.},
  langid = {english},
  keywords = {Fatty acids,Human milk,Lactational stages,Pooled data,Preterm,Term},
  file = {C:\Users\user\OneDrive_HU\Zotero\1_Paper\Press\2025_MCthesis\Floris et al. - 2020 - Human milk fatty acid profile across lactational stages after term and preterm delivery A pooled da.pdf}
}

@article{goldshteinBreastfeedingDurationChild2025,
  title = {Breastfeeding {{Duration}} and {{Child Development}}},
  author = {Goldshtein, Inbal and Sadaka, Yair and Amit, Guy and Kasir, Nitsa and Bourgeron, Thomas and Warrier, Varun and Akiva, Pinchas and Avgil Tsadok, Meytal and Zimmerman, Deena R.},
  year = 2025,
  month = mar,
  journal = {JAMA Netw Open},
  volume = {8},
  number = {3},
  pages = {e251540},
  issn = {2574-3805},
  doi = {10.1001/jamanetworkopen.2025.1540},
  urldate = {2026-01-02},
  abstract = {Detecting and addressing potentially modifiable factors associated with healthy development is key to optimizing a child's potential. When investigating the outcomes of child development, it is important to account for disparities in feeding practices and avoid confounding bias.To estimate the independent association between breastfeeding and attainment of developmental milestones or neurodevelopmental conditions.This retrospective cohort study used data from a national network for routine child development surveillance in Israel linked with national social insurance financial entitlements for neurodevelopmental deficiencies. Participants were children born between January 2014 and December 2020 after at least 35 weeks' gestation without severe morbidity and with at least 1 follow-up surveillance visit at 2 to 3 years of age. Outcome data were collected in March 2023.Duration and exclusivity of breastfeeding in infancy.The primary outcomes were delays in attainment of developmental milestones and diagnosis of prespecified neurodevelopmental conditions. Multivariable regression, matching, and within-family analyses were used to estimate adjusted odds ratios (AORs) after accounting for potential confounding factors related to the child (gestational age, birth weight, multiple gestation, and child order in the family) and mother (age, socioeconomic status, educational level, marital status, employment, nationality, and postpartum depression).Of 570\,532 children (291\,953 [51.2\%] male), 20 642 (3.6\%) were preterm, 38\,499 (6.7\%) were small for gestational age, and 297\,571 (52.1\%) were breastfed for at least 6 months (123\,984 [41.7\%] were exclusively breastfed). Children who were breastfed for at least 6 months exhibited fewer delays in attaining language and social or motor developmental milestones compared with children exposed to less than 6 months of breastfeeding (AOR, 0.73 [95\% CI, 0.71-0.76] for exclusive breastfeeding; AOR, 0.86 [95\% CI, 0.83-0.88] for nonexclusive breastfeeding). Among 37\,704 sibling pairs, children who were breastfed for at least 6 months were less likely to demonstrate milestone attainment delays (OR, 0.91 [95\% CI, 0.86-0.97]) or be diagnosed with neurodevelopmental conditions (OR, 0.73 [95\% CI, 0.66-0.82]) compared with their sibling with less than 6 months of breastfeeding or no breastfeeding.In this cohort study, exclusive or longer duration of breastfeeding was associated with reduced odds of developmental delays and language or social neurodevelopmental conditions. These findings may guide parents, caregivers, and public health initiatives in promoting early child development.},
  langid = {english},
  annotation = {abstractTranslation: 子供の健全な発達に関連する、修正可能な要因を検出し、対処することは、子供の潜在能力を最大限に引き出す鍵となります。子供の発達の成果を調査する際には、授乳習慣の違いを考慮し、交絡バイアスを避けることが重要です。母乳育児と発達上の到達点あるいは神経発達症との独立した関連性を推定すること。この後ろ向きコホート研究では、イスラエルのルーチン小児発達サーベイランスのための全国ネットワークのデータと、神経発達障害に対する社会保険の給付データを照合して使用しました。参加者は、2014年1月から2020年12月の間に生まれた、在胎35週以上で重度の罹患がなく、2歳から3歳までに少なくとも1回のフォローアップサーベイランス受診があった子供たちです。アウトカムデータは2023年3月に収集されました。乳児期の母乳育児期間と母乳独占期間。主要なアウトカムは、発達上の到達点の遅延と、規定済みの神経発達症の診断でした。多変量回帰、マッチング、および家族内分析を用いて、子供（在胎週数、出生体重、多胎妊娠、および家族内の出生順位）および母親（年齢、社会経済的地位、教育レベル、婚姻状況、雇用、国籍、および産後うつ病）に関連する潜在的な交絡因子を考慮した後、調整オッズ比（AOR）を推定しました。570,532人の子供（男性291,953人［51.2％］）のうち、20,642人（3.6％）は早産、38,499人（6.7％）は在胎週数に対して小さく、297,571人（52.1％）は少なくとも6ヶ月間母乳育児を受けていました（123,984人［41.7％］は母乳独占育児でした）。少なくとも6ヶ月間母乳育児を受けた子供は、6ヶ月未満の母乳育児を受けた子供と比較して、言語および社会的または運動発達上の到達点の遅延が少なかった（母乳独占育児の場合、AOR、0.73［95％CI、0.71-0.76］；非独占母乳育児の場合、AOR、0.86［95％CI、0.83-0.88］）ことを示しました。37,704組の兄弟姉妹ペアのうち、少なくとも6ヶ月間母乳育児を受けた子供は、6ヶ月未満の母乳育児または母乳育児なしの兄弟姉妹と比較して、発達上の到達点の遅延を示す可能性（OR、0.91［95％CI、0.86-0.97］）あるいは神経発達症と診断される可能性（OR、0.73［95％CI、0.66-0.82］）が低かった。このコホート研究では、母乳独占育児またはより長い期間の母乳育児は、発達の遅延および言語または社会神経発達症のオッズの低下と関連していました。これらの発見は、親、保護者、および公衆衛生の取り組みが、早期の子供の発達を促進する上で指針となる可能性があります。},
  file = {C:\Users\user\OneDrive_HU\Zotero\1_Paper\Press\2025_MCthesis\Goldshtein et al. - 2025 - Breastfeeding Duration and Child Development.pdf}
}

@article{hansonDevelopmentalOriginsHealth2015,
  title = {Developmental Origins of Health and Disease -- {{Global}} Public Health Implications},
  author = {Hanson, M. A. and Gluckman, P. D.},
  year = 2015,
  month = jan,
  journal = {Best Practice \& Research Clinical Obstetrics \& Gynaecology},
  series = {Non-{{Communicable Diseases}} in {{Maternal Fetal Medicine}}: {{Volume I}}},
  volume = {29},
  number = {1},
  pages = {24--31},
  issn = {1521-6934},
  doi = {10.1016/j.bpobgyn.2014.06.007},
  urldate = {2026-01-05},
  abstract = {The rapidly rising prevalence of non-communicable diseases (NCDs) represents a major challenge to public health and clinical medicine globally. NCDs are increasing rapidly in high-income countries, but even more rapidly in some low-middle-income countries with insufficient resources to meet the challenge. Whilst not identified in the Millennium Development Goals, there is much attention paid to NCDs in the discussions at many levels on the Sustainable Development Goals, as they underpin economic, social and environmental development in the post-2015 era. In this article, we discuss how a life-course approach to health, commencing of necessity in early development, can provide new opportunities for addressing this challenge. The approach can leverage human health capital throughout life and across generations. New insights into mechanisms, especially those processes by which the developmental environment affects epigenetic processes in the developing offspring, offer the prospect of identifying biomarkers of future risks. New interventions to promote health literacy, lifestyle and physical fitness in adolescents, young adults and their children hold great promise. In this respect, health-care professionals concerned with preconceptional, pregnancy and newborn care will have a vital role to play.},
  keywords = {epigenetic,health literacy,non-communicable disease},
  file = {C:\Users\user\OneDrive_HU\Zotero\1_Paper\Press\2025_MCthesis\Hanson と Gluckman - 2015 - Developmental origins of health and disease – Global public health implications.pdf}
}

@article{jamesMetabolismPredominantHuman2019a,
  title = {Metabolism of the Predominant Human Milk Oligosaccharide Fucosyllactose by an Infant Gut Commensal},
  author = {James, Kieran and Bottacini, Francesca and Contreras, Jose Ivan Serrano and Vigoureux, Mariane and Egan, Muireann and Motherway, Mary O'connell and Holmes, Elaine and {van Sinderen}, Douwe},
  year = 2019,
  month = oct,
  journal = {Sci Rep},
  volume = {9},
  number = {1},
  pages = {15427},
  publisher = {Nature Publishing Group},
  issn = {2045-2322},
  doi = {10.1038/s41598-019-51901-7},
  urldate = {2026-01-07},
  abstract = {A number of bifidobacterial species are found at a particularly high prevalence and abundance in faecal samples of healthy breastfed infants, a phenomenon that is believed to be, at least partially, due to the ability of bifidobacteria to metabolize Human Milk Oligosaccharides (HMOs). In the current study, we isolated a novel strain of Bifidobacterium kashiwanohense, named APCKJ1, from the faeces of a four-week old breastfed infant, based on the ability of the strain to utilise the HMO component fucosyllactose. We then determined the full genome sequence of this strain, and employed the generated data to analyze fucosyllactose metabolism in B. kashiwanohense APCKJ1. Transcriptomic and growth analyses, combined with metabolite analysis, in vitro hydrolysis assays and heterologous expression, allowed us to elucidate the pathway for fucosyllactose metabolism in B. kashiwanohense APCKJ1. Homologs of the key genes for this metabolic pathway were identified in particular in infant-derived members of the Bifdobacterium genus, revealing the apparent niche-specific nature of this pathway, and allowing a broad perspective on bifidobacterial fucosyllactose and L-fucose metabolism.},
  copyright = {2019 The Author(s)},
  langid = {english},
  keywords = {Bacterial genomics,Cellular microbiology},
  file = {C:\Users\user\OneDrive_HU\Zotero\1_Paper\Press\2025_MCthesis\James et al. - 2019 - Metabolism of the predominant human milk oligosaccharide fucosyllactose by an infant gut commensal.pdf}
}

@article{jovelMetagenomicsMetatranscriptomicsMurine2022,
  title = {Metagenomics {{Versus Metatranscriptomics}} of the {{Murine Gut Microbiome}} for {{Assessing Microbial Metabolism During Inflammation}}},
  author = {Jovel, Juan and Nimaga, Aissata and Jordan, Tracy and O'Keefe, Sandra and Patterson, Jordan and Thiesen, Aducio and Hotte, Naomi and {Bording-Jorgensen}, Michael and Subedi, Sudip and Hamilton, Jessica and Carpenter, Eric J. and Lauga, B{\'e}atrice and Elahi, Shokrollah and Madsen, Karen L. and Wong, Gane Ka-Shu and Mason, Andrew L.},
  year = 2022,
  month = feb,
  journal = {Front. Microbiol.},
  volume = {13},
  publisher = {Frontiers},
  issn = {1664-302X},
  doi = {10.3389/fmicb.2022.829378},
  urldate = {2026-01-02},
  abstract = {Shotgun metagenomics studies have improved our understanding of microbial population dynamics and have revealed significant contributions of microbes to gut homeostasis. They also allow in silico inference of the metagenome. While they link the microbiome with metabolic abnormalities associated with disease phenotypes, they do not capture microbial gene expression patterns that occur in response to the multitude of stimuli that constantly ambush the gut environment. Metatranscriptomics closes that gap, but its implementation is more expensive and tedious. We assessed the metabolic perturbations associated with gut inflammation using shotgun metagenomics and metatranscriptomics. Shotgun metagenomics detected changes in abundance of bacterial taxa known to be SCFA producers, which favors gut homeostasis. Bacteria in the phylum Firmicutes were found at decreased abundance, while those in phyla Bacteroidetes and Proteobacteria were found at increased abundance. Surprisingly, inferring the coding capacity of the microbiome from shotgun metagenomics data did not result in any statistically significant difference, suggesting functional redundancy in the microbiome or poor resolution of shotgun metagenomics data to profile bacterial pathways, especially when sequencing is not very deep. Obviously, the ability of metatranscriptomics libraries to detect transcripts expressed at basal (or simply low) levels is also dependent on sequencing depth. Nevertheless, metatranscriptomics informed about contrasting roles of bacteria during inflammation. Functions involved in nutrient transport, immune suppression and regulation of tissue damage were dramatically upregulated, perhaps contributed by homeostasis-promoting bacteria. Functions ostensibly increasing bacteria pathogenesis were also found upregulated, perhaps as a consequence of increased abundance of Proteobacteria. Bacterial protein synthesis appeared downregulated. In summary, shotgun metagenomics was useful to profile bacterial population composition and taxa relative abundance, but did not inform about differential gene content associated with inflammation. Metatranscriptomics was more robust for capturing bacterial metabolism in real time. Although both approaches are complementary, it is often not possible to apply them in parallel. We hope our data will help researchers to decide which approach is more appropriate for the study of different aspects of the microbiome.},
  langid = {english},
  keywords = {bacterial metabolic pathways,gut inflammation-on-a-chip,metatranscriptomics,microbiome,Shotgun metagenomics},
  annotation = {abstractTranslation: ショットガンメタゲノミクス研究により、微生物集団動態の理解が深まり、腸管恒常性に対する微生物の重要な寄与が明らかになりました。また、メタゲノムのインシリコ推論も可能になります。これらは、マイクロバイオームと疾患の表現型に関連する代謝異常とを関連付けますが、腸内環境を絶えず待ち伏せする多数の刺激に応答して発生する微生物の遺伝子発現パターンを捕捉することはできません。メタトランスクリプトミクスはそのギャップを埋めますが、その実装はより高価で退屈です。私たちは、ショットガンメタゲノミクスとメタトランスクリプトミクスを使用して、腸の炎症に関連する代謝の混乱を評価しました。ショットガンメタゲノミクスにより、腸の恒常性を促進するSCFA生産者として知られる細菌分類群の存在量の変化が検出されました。ファーミクテス門の細菌の存在量は減少しましたが、バクテロイデス門とプロテオバクテリア門の細菌の存在量は増加しました。驚くべきことに、ショットガンメタゲノミクスデータからマイクロバイオームのコーディング能力を推測しても、統計的に有意な差は生じず、特に配列決定がそれほど深くない場合には、マイクロバイオームの機能的冗長性、または細菌経路をプロファイリングするためのショットガンメタゲノミクスデータの解像度が低いことを示唆しています。明らかに、基礎レベル (または単に低レベル) で発現された転写物を検出するメタトランスクリプトミクス ライブラリーの能力は、シーケンシングの深度にも依存します。それにもかかわらず、メタトランスクリプトミクスは、炎症中の細菌の対照的な役割について情報を与えた。栄養素の輸送、免疫抑制、組織損傷の制御に関与する機能は、おそらく恒常性を促進する細菌によって寄与されて、劇的に上方制御されました。おそらくプロテオバクテリアの存在量が増加した結果として、表面上細菌の病因を増加させる機能も上方制御されていることが判明した。細菌のタンパク質合成は下方制御されているようです。要約すると、ショットガン メタゲノミクスは細菌集団の組成と分類群の相対的な存在量をプロファイリングするのに役立ちましたが、炎症に関連する異なる遺伝子含有量については情報を提供しませんでした。メタトランスクリプトミクスは、細菌の代謝をリアルタイムで捕捉する場合により堅牢でした。両方のアプローチは補完的ですが、多くの場合、それらを並行して適用することはできません。私たちのデータが、研究者がマイクロバイオームのさまざまな側面の研究にどのアプローチがより適切であるかを決定するのに役立つことを願っています。},
  file = {C:\Users\user\OneDrive_HU\Zotero\1_Paper\Press\2025_MCthesis\Jovel et al. - 2022 - Metagenomics Versus Metatranscriptomics of the Murine Gut Microbiome for Assessing Microbial Metabol.pdf}
}

@article{kimComponentsHumanBreast2020,
  title = {Components of Human Breast Milk: From Macronutrient to Microbiome and {{microRNA}}},
  shorttitle = {Components of Human Breast Milk},
  author = {Kim, Su Yeong and Yi, Dae Yong},
  year = 2020,
  month = aug,
  journal = {Clin Exp Pediatr},
  volume = {63},
  number = {8},
  pages = {301--309},
  issn = {2713-4148},
  doi = {10.3345/cep.2020.00059},
  urldate = {2023-09-20},
  abstract = {Human breast milk (HBM) is essential for the infant's growth and development right after birth and is an irreplaceable source of nutrition for early human survival. Various infant formulas have many similarities to HBM in many components, but there is no perfect substitute for HBM. Recently, various breast milk components and their roles have been studied according to the development of various analysis techniques. As is already well known, HBM contains about 87\%--88\% water, and 124- g/L solid components as macronutrients, including about 7\% (60--70 g/L) carbohydrates, 1\% (8--10 g/L) protein, and 3.8\% (35--40 g/L) fat. The composition may vary depending on the environmental factors, including maternal diet. Colostrum is low in fat but high in protein and relatively rich in immuneprotective components. Although HBM contains enough vitamins to ensure normal growth of the infant, vitamins D and K may be insufficient, and the infant may require their supplementation. Growth factors in HBM also serve as various bioactive proteins and peptides on the intestinal tract, vasculature, nervous system, and endocrine system. In the past, HBM of a healthy mother was thought to be sterile. However, several subsequent studies have confirmed the presence of rich and diverse microbial communities in HBM. Some studies suggested that the genera \emph{Staphylococcus} and \emph{Streptococcus} may be universally predominant in HBM, but the origin of microbiota still remains controversial. Lastly, milk is the one of most abundant body fluid of microRNAs, which are known to play a role in various functions, such as immunoprotection and developmental programming, through delivering from HBM and absorption by intestinal epithelial cells. In conclusion, HBM is the most important source of nutrition for infants and includes microbiomes and miRNAs for growth, development, and immunity.},
  langid = {english},
  keywords = {hbm,review},
  file = {C:\Users\user\OneDrive_HU\Zotero\EMPTY_COLLECTION_NAME\Components of human breast milk_2020.pdf}
}

@article{kodaMolecularBasisSecretor1996,
  title = {Molecular Basis for Secretor Type Alpha(1,2)-Fucosyltransferase Gene Deficiency in a {{Japanese}} Population: A Fusion Gene Generated by Unequal Crossover Responsible for the Enzyme Deficiency.},
  shorttitle = {Molecular Basis for Secretor Type Alpha(1,2)-Fucosyltransferase Gene Deficiency in a {{Japanese}} Population},
  author = {Koda, Y. and Soejima, M. and Liu, Y. and Kimura, H.},
  year = 1996,
  month = aug,
  journal = {Am J Hum Genet},
  volume = {59},
  number = {2},
  pages = {343--350},
  issn = {0002-9297},
  urldate = {2025-08-06},
  abstract = {About 20\%-25\% of Caucasian individuals are nonsecretors who fail to express soluble A, B, H, and Lewis b histo-blood group antigens in secretory organs and secretory fluids because of the absence of the Secretor gene (FUT2)-encoded alpha(1,2)-fucosyltransferase (Se enzyme) activity. Recently, the FUT2 and a pseudogene have been isolated, and an Se enzyme-deficient allele (se) caused by a nonsense mutation (G428A, se1) in Caucasians has also been reported. Although we were unable to find the se1 allele, we have found a missense mutation (A385T, se2) and two nonsense mutations (C571T, se3 and C628T, se4) in the Japanese Se enzyme-deficient alleles. In addition, we have found a fusion gene, which consisted of the 5'-region of the pseudogene and the 3'-region of the functional FUT2, as a Se enzyme-deficient allele (se5). The DNA sequence analysis of the fusion gene indicated that the crossover region corresponded to regions between bases 253 and 313 of the pseudogene and between bases 211 and 271 of the FUT2. This finding suggested that the fusion gene was generated by homologous but unequal crossover. A population study on 141 randomly selected Japanese has indicated that the se2 is a common Se enzyme-deficient allele in the Japanese population. The results suggest that Se enzyme-deficient alleles are race specific.},
  pmcid = {PMC1914724},
  pmid = {8755920},
  file = {C:\Users\user\OneDrive_HU\Zotero\1_Paper\BM\Koda et al. - 1996 - Molecular basis for secretor type alpha(1,2)-fucosyltransferase gene deficiency in a Japanese popula.pdf}
}

@article{komatsuAssociationsMaternalDiet2023,
  title = {Associations between {{Maternal Diet}}, {{Human Milk Macronutrients}}, and {{Breast-Fed Infant Growth}} during the {{First Month}} of {{Life}} in the {{SMILE Iwamizawa}} in {{Japan}}},
  author = {Komatsu, Yosuke and Wada, Yasuaki and Tabata, Fuka and Kawakami, Satomi and Takeda, Yasuhiro and Nakamura, Kiminori and Ayabe, Tokiyoshi and Nakamura, Koshi and Kimura, Takashi and Tamakoshi, Akiko},
  year = 2023,
  month = jan,
  journal = {Nutrients},
  volume = {15},
  number = {3},
  pages = {654},
  publisher = {Multidisciplinary Digital Publishing Institute},
  issn = {2072-6643},
  doi = {10.3390/nu15030654},
  urldate = {2023-09-15},
  abstract = {Maternal diet may affect human milk macronutrients, but it remains to be elucidated whether this is also influential in infant growth. This study aimed to examine (1) how maternal diet influences human milk macronutrients, and (2) to what extent the variation in milk macronutrients affects infant growth during the first month of life. In 71 Japanese lactating women, maternal dietary information was collected from the brief-type self-administered diet history questionnaire, and anthropometry of mother--infant dyads was collected from medical records. Macronutrients in milk were analyzed by a Human Milk Analyzer. Maternal retinol intake was associated with the carbohydrate content in human milk at 1-month postpartum (standardized {$\beta$} coefficient: 0.287; p = 0.038). Moreover, the energy content in human milk was associated with an increase in the weight standard deviation score based on the WHO growth standard at 1 month of age (standardized {$\beta$} coefficient: 0.399; p = 0.046). Nevertheless, the milk macronutrient was not associated with the risk of infant growth abnormalities. In conclusion, a part of the maternal diet impacts macronutrient contents in human milk, but milk macronutrients have a limited effect on infant growth only within the normal growth curve during the first month of life.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  langid = {english},
  keywords = {coi},
  annotation = {abstractTranslation: 母乳中のマクロ栄養素は母体の食事に影響される可能性があるが、それが乳児の成長にも影響するかどうかは解明されていない。本研究は、(1) 母体の食事が母乳中のマクロ栄養素にどのように影響するか、(2) 母乳中のマクロ栄養素の変動が、生後1ヶ月における乳児の成長にどの程度影響するかを検討することを目的とした。71人の日本の授乳婦を対象に、簡略型自記式食事歴質問票から母体の食事情報を、母子ペアの身体計測を医療記録から収集した。母乳中のマクロ栄養素はHuman Milk Analyzerを用いて分析した。母体のレチノール摂取量は、産後1ヶ月時の母乳中の炭水化物量と関連していた（標準化{$\beta$}係数：0.287；p = 0.038）。さらに、母乳中のエネルギー量は、WHO成長基準に基づく1ヶ月齢時の体重標準偏差得点の上昇と関連していた（標準化{$\beta$}係数：0.399；p = 0.046）。しかしながら、母乳中のマクロ栄養素は、乳児の成長異常のリスクとは関連していなかった。結論として、母体の食事の一部は母乳中のマクロ栄養素組成に影響を与えるが、母乳中のマクロ栄養素は、生後1ヶ月において正常成長曲線内でのみ、乳児の成長に限定的な影響を与える。},
  file = {C:\Users\user\OneDrive_HU\Zotero\Press\241001_NMR討論会\Associations between Maternal Diet, Human Milk Macronutrients, and Breast-Fed_2023.pdf}
}

