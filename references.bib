@article{poulsenDynamicChangesHuman2022,
  title = {Dynamic {{Changes}} in the {{Human Milk Metabolome Over}} 25 {{Weeks}} of {{Lactation}}},
  author = {Poulsen, Katrine Overgaard and Meng, Fanyu and Lanfranchi, Elisa and Young, Jette Feveile and Stanton, Catherine and Ryan, C. Anthony and Kelly, Alan L. and Sundekilde, Ulrik Kraemer},
  year = 2022,
  month = jul,
  journal = {Front. Nutr.},
  volume = {9},
  pages = {917659},
  issn = {2296-861X},
  doi = {10.3389/fnut.2022.917659},
  urldate = {2024-10-20},
  abstract = {Human milk (HM) provides essential nutrition for ensuring optimal infant growth and development postpartum. Metabolomics offers insight into the dynamic composition of HM. Studies have reported the impact of lactation stage, maternal genotype, and gestational age on HM metabolome. However, the majority of the studies have considered changes within the first month of lactation or sampled with large intervals. This leaves a gap in the knowledge of progressing variation in HM composition beyond the first month of lactation. The objective of this study was to investigate whether the HM metabolome from mothers with term deliveries varies beyond 1 month of lactation, during the period in which HM is considered fully mature. Human milk samples (n = 101) from 59 mothers were collected at weeks 1--2, 3--5, 7--9, and 20--25 postpartum and analyzed using 1H nuclear magnetic resonance spectroscopy. Several metabolites varied over lactation and exhibited dynamic changes between multiple time points. Higher levels of HM oligosaccharides, cis-aconitate, O-phosphocholine, O-acetylcarnitine, gluconate, and citric acid were observed in early lactation, whereas later in lactation, levels of lactose, 3-fucosyllactose, glutamine, glutamate, and shortand medium-chain fatty acids were increased. Notably, we demonstrate that the HM metabolome is dynamic during the period of maturity.},
  langid = {english},
  file = {C\:\\Users\\user\\OneDrive_HU\\Zotero\\coi\\Dynamic Changes in the Human Milk Metabolome Over 25 Weeks of Lactation_22.pdf;C\:\\Users\\user\\OneDrive_HU\\Zotero\\coi\\Poulsen et al. - 2022 - Dynamic Changes in the Human Milk Metabolome Over 25 Weeks of Lactation.pdf}
}

