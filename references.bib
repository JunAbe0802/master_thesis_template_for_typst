@article{satoAbsoluteQuantificationEight2024a,
  title = {Absolute Quantification of Eight Human Milk Oligosaccharides in Breast Milk to Evaluate Their Concentration Profiles and Associations with Infants' Neurodevelopmental Outcomes},
  author = {Sato, Keigo and Nakamura, Yoshitaka and Fujiyama, Kazuhito and Ohneda, Kinuko and Nobukuni, Takahiro and Ogishima, Soichi and Mizuno, Satoshi and Koshiba, Seizo and Kuriyama, Shinichi and Jinno, Shinji},
  year = 2024,
  journal = {Journal of Food Science},
  volume = {89},
  number = {12},
  pages = {10152--10170},
  issn = {1750-3841},
  doi = {10.1111/1750-3841.17597},
  urldate = {2026-01-02},
  abstract = {Human milk oligosaccharides (HMOs) have been positively associated with child neurodevelopment in some cohort studies. However, there is a lack of consistency in the association between HMOs and benefits to infants' brains. Moreover, the quantification methods for HMOs have not yet been standardized. In this study, we developed a quantification method for evaluating eight HMOs (2{$\prime$}-fucosyllactose [2{$\prime$}-FL], 3{$\prime$}-fucosyllactose [3{$\prime$}-FL], 3{$\prime$}-sialyllactose [3{$\prime$}-SL], 6{$\prime$}-sialyllactose [6{$\prime$}-SL], lactosialyltetrasaccharide a [LSTa], lactosialyltetrasaccharide b [LSTb], lactosialyltetrasaccharide c [LSTc], and disialyllacto-N-tetraose [DSLNT]) in breast milk. After validating the method, we applied it to 1-month breast milk samples (n = 150) from the Tohoku Medical Megabank Project Birth and Three-Generation Cohort Study to assess HMO profiles in breast milk and their possible association with changes in head circumference z-score ({$\Delta$}HCZ) and neurodevelopmental scores of children (as measured by the Ages and Stages Questionnaire, Third Edition). The validation demonstrated that the method had relative standard deviation {$\leq$} 12.7\% of precision and 79.5--110.9\% of accuracy. Using this method, eight HMO levels (2{$\prime$}-FL, 0--4.74 mg/mL; 3{$\prime$}-FL, 0.02--1.52 mg/mL; 3{$\prime$}-SL, 0.07--0.32 mg/mL; 6{$\prime$}-SL, 0.01--0.70 mg/mL; LSTa, 0.002--0.043 mg/mL; LSTb, 0.02--0.31 mg/mL; LSTc, 0.001--0.47 mg/mL; and DSLNT, 0.09--0.71 mg/mL [min--max, all participants]) and the ratio of low secretors (16.0\%) in the Japanese cohort were obtained. The obtained HMO levels in breast milk were subjected to multivariate analysis to screen for HMOs showing a positive association with {$\Delta$}HCZ and neurodevelopmental scores. The results proposed that {$\Delta$}HCZ was positively associated with LSTb and 2{$\prime$}-FL levels, whereas neurodevelopmental scores were positively associated with 2{$\prime$}-FL levels (among all participants) and 3{$\prime$}-SL and DSLNT levels (among secretor participants). This study showed that the developed method provides HMO profiles in Japanese breast milk, as well as additional information on the associations between specific HMOs and neurodevelopment, reinforcing the sum of evidence for the role of HMOs in the brain.},
  copyright = {\copyright{} 2024 The Author(s). Journal of Food Science published by Wiley Periodicals LLC on behalf of Institute of Food Technologists.},
  langid = {english},
  keywords = {breast milk,head growth,human milk oligosaccharides,infants,neurodevelopment},
  annotation = {abstractTranslation: ヒト母乳オリゴ糖（HMO）は、一部のコホート研究において小児の神経発達と正の関連が示唆されてきました。しかし、HMOと乳児の脳への有益性との関連については、一貫性に欠けています。さらに、HMOの定量化手法はまだ標準化されていません。本研究では、母乳中の8種類のHMO（2{$\prime$}-フコシルラクトース[2{$\prime$}-FL]、3{$\prime$}-フコシルラクトース[3{$\prime$}-FL]、3{$\prime$}-シアリルラクトース[3{$\prime$}-SL]、6{$\prime$}-シアリルラクトース[6{$\prime$}-SL]、ラクトシアリルトリテトラサッカライドa[LSTa]、ラクトシアリルトリテトラサッカライドb[LSTb]、ラクトシアリルトリテトラサッカライドc[LSTc]、ジシアリルラクト-N-テトラオース[DSLNT]）を評価するための定量化手法を開発しました。手法の検証後、東北メディカル・メガバンク計画出生コホートおよび三世代コホート研究から得られた1ヶ月の母乳サンプル（n = 150）に適用し、母乳中のHMOプロファイルと、頭囲zスコアの変化（{$\Delta$}HCZ）および小児の神経発達スコア（Ages and Stages Questionnaire, Third Editionで測定）との関連性を評価しました。検証により、本手法の精度は相対標準偏差が12.7\%以下、精度は79.5〜110.9\%であることが実証されました。この手法を用いることにより、8種類のHMOレベル（2{$\prime$}-FL、0--4.74 mg/mL；3{$\prime$}-FL、0.02--1.52 mg/mL；3{$\prime$}-SL、0.07--0.32 mg/mL；6{$\prime$}-SL、0.01--0.70 mg/mL；LSTa、0.002--0.043 mg/mL；LSTb、0.02--0.31 mg/mL；LSTc、0.001--0.47 mg/mL；DSLNT、0.09--0.71 mg/mL [最小--最大、全参加者]）および日本人コホートにおける低分泌者（secretor）の割合（16.0\%）が得られました。得られた母乳中のHMOレベルは、{$\Delta$}HCZおよび神経発達スコアとの正の関連を示すHMOをスクリーニングするために多変量解析に供されました。その結果、{$\Delta$}HCZはLSTbおよび2{$\prime$}-FLレベルと正の関連を示し、一方、神経発達スコアは2{$\prime$}-FLレベル（全参加者）および3{$\prime$}-SL、DSLNTレベル（分泌者参加者）と正の関連を示すことが示唆されました。本研究は、開発された手法が日本人母乳中のHMOプロファイルを提供するだけでなく、特定のHMOと神経発達との関連性に関する追加情報も提供することを示し、脳におけるHMOの役割に関する証拠の蓄積を強化しました。},
  file = {C:\Users\user\OneDrive_HU\Zotero\1_Paper\Press\2025_MCthesis\Sato et al. - 2024 - Absolute quantification of eight human milk oligosaccharides in breast milk to evaluate their concen.pdf}
}

@article{stiemsmaRoleMicrobiomeDevelopmental2018,
